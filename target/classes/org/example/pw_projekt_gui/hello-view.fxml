    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <HBox fillHeight="false" prefHeight="30.0" prefWidth="59.0">
         <children>
            <Button fx:id="StartBtn" mnemonicParsing="false" onAction="#onStartButtonClick" text="Start">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="StopBtn" mnemonicParsing="false" onAction="#onStopButtonClick" text="Stop">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="SettingsBtn" mnemonicParsing="false" onAction="#onSettingsButtonClick" text="Ustawienia">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="ExitBtn" mnemonicParsing="false" onAction="#onExitButtonClick" text="WyjdÅº" />
         </children>
      </HBox>
      <Pane fx:id="Pejn" minHeight="707.0" minWidth="1040.0" prefHeight="707.0" prefWidth="1040.0" />
   </children>
</VBox>
